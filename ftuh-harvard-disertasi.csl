  <ns0:locale xml:lang="id">
    <ns0:terms>
      <ns0:term name="page" form="short">
        <ns0:single>hal.</ns0:single>
        <ns0:multiple>hal.</ns0:multiple>
      </ns0:term>
      <ns0:term name="retrieved">diakses</ns0:term>
      <ns0:term name="from">dari</ns0:term>
      <ns0:term name="accessed">diakses tanggal</ns0:term>
      <ns0:term name="open-quote">‘</ns0:term>
      <ns0:term name="close-quote">’</ns0:term>
      <ns0:term name="et-al">dkk.</ns0:term>
    </ns0:terms>
